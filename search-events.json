---
layout: null
---
[
  {% for post in site.events reversed %}
    {
      "title"    : "{{ post.name }}",
      "category" : "{{ post.tools | join: ', '}}",
      "url"      : "{{ post.external_url | default: post.url | relative_url }}",
      "date"     : "{{ post.date }}",
      "content"  : "{{ post.tools | join: ', '}}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]