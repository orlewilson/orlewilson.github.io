---
layout: null
---
[
  {% for post in site.events reversed %}
    {
      "title"    : "{{ post.name }}",
      "category" : "{{ post.category }}",
      "url"      : "{{ post.external_url | default: post.url | relative_url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]